<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Amazon Regression Test Suite" thread-count="2" parallel ="tests" verbose ="3">


<listeners>
<listener class-name="com.qa.amazon.listeners.ExtentReportListener"></listener>
</listeners>


<test name="Amazon Regression Test_LandingPage_Chrome">

     <parameter name="browser" value="chrome">
     </parameter>
     
    	
		<classes>
			<class name="com.qa.amazon.tests.LandingPageTest" />
        </classes>
	</test>
	
	
	
	
	
   <test name="Amazon Regression Test_LandingPage_Edge">

     <parameter name="browser" value="edge">
     </parameter>
     
    	
		<classes>
			<class name="com.qa.amazon.tests.LandingPageTest" />
        </classes>
   </test>

</suite> 